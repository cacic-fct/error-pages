---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="pt-BR">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<meta name="theme-color" content="#dfdfdf" />
	</head>
	<body>
		<div class="container">
			<img
				class="mouse"
				alt="Ratinho apagando um incêncio em um computador"
				src="data:image/avif;base64, "
			/>

			<slot />

			<footer>
				<p class="footnote">
					<a href="https://cacic.dev.br/docs">CACiC Docs</a>
					•
					<a href="https://status.cacic.dev.br">Status Page</a>
				</p>
			</footer>
		</div>
	</body>
</html>

<style>
	body {
		background-color: #dfdfdf;
		font-family:
			system-ui,
			-apple-system,
			BlinkMacSystemFont,
			'Segoe UI',
			Roboto,
			Oxygen,
			Ubuntu,
			Cantarell,
			'Open Sans',
			'Helvetica Neue',
			sans-serif;
		display: flex;
		justify-content: center;
		align-items: center;
		height: 100vh;
		margin: 0;
	}
	.container {
		max-width: 860px;
	}

	.mouse {
		max-width: 400px;
		width: 100%;
		height: auto;
		display: block;
		margin: 0 auto;
	}
	.footnote {
		font-size: 0.8rem;
		font-weight: 300;
		margin-top: 2rem;
	}

	.footnote a {
		/* no link clicked decoration */
		color: #000;
		text-decoration: none;
	}

	.footnote a:hover {
		text-decoration: underline;
	}

	footer {
		padding: 1rem;
		text-align: center;
	}
</style>
